{
  "name": "genshin-scrapper",
  "version": "1.0.0",
  "description": "A bunch of scripts that parse Genshin Impact data",
  "main": "index.ts",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "start:source": "npx tsc && node out/index.ts",
    "build": "npx tsc",
    "watch": "npx tsc ./src/index.ts --watch",
    "lint": "npx eslint ./src/*.ts",
    "lint:fix": "npx eslint ./src/*.ts --fix",
    "format": "npx prettier ./src/*.ts --write"
  },
  "keywords": [
    "genshin",
    "genshin-impact"
  ],
  "author": "Ilya Revenko <ilya.revenko17@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/node": "^14.14.40",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.6",
    "request": "^2.88.2",
    "webp-converter": "^2.3.3"
  }
}
